e":"utils/fetch/interceptors.js","original":{"line":57,"column":35}},{"generated":{"line":61,"column":36},"source":"utils/fetch/interceptors.js","original":{"line":57,"column":36}},{"generated":{"line":62,"column":6},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":6},"name":"errorCallback"},{"generated":{"line":62,"column":19},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":19}},{"generated":{"line":62,"column":23},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":23},"name":"interceptorsResError"},{"generated":{"line":62,"column":43},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":43}},{"generated":{"line":62,"column":44},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":44},"name":"push"},{"generated":{"line":62,"column":48},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":48}},{"generated":{"line":62,"column":49},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":49},"name":"errorCallback"},{"generated":{"line":62,"column":62},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":62}},{"generated":{"line":62,"column":63},"source":"utils/fetch/interceptors.js","original":{"line":58,"column":63}},{"generated":{"line":63,"column":4},"source":"utils/fetch/interceptors.js","original":{"line":59,"column":4}},{"generated":{"line":64,"column":2},"source":"utils/fetch/interceptors.js","original":{"line":60,"column":2}},{"generated":{"line":65,"column":0},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":0}},{"generated":{"line":65,"column":1},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":1}},{"generated":{"line":66,"column":0},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2},"name":"exports"},{"generated":{"line":66,"column":7},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2}},{"generated":{"line":66,"column":8},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2},"name":"interceptors"},{"generated":{"line":66,"column":20},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2}},{"generated":{"line":66,"column":23},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2},"name":"interceptors"},{"generated":{"line":66,"column":35},"source":"utils/fetch/interceptors.js","original":{"line":61,"column":2}}],"sources":{"utils/fetch/interceptors.js":"// 定义用来存储拦截请求和拦截响应结果的处理和错误结果处理的函数集合\r\nlet interceptorsReq = [];\r\nlet interceptorsReqError = [];\r\nlet interceptorsRes = [];\r\nlet interceptorsResError = [];\r\n\r\nconst OriginFetch = window.fetch;\r\n\r\nfunction Cfetch(input, init = {}) {\r\n  // interceptorsReq是拦截请求的拦截处理函数集合\r\n  interceptorsReq.forEach((item) => {\r\n    init = item(init);\r\n  });\r\n\r\n  // 在原生fetch外面封装一个promise，为了在promise里面可以对fetch请求的结果做拦截处理。\r\n  // 同时，保证Cfetch函数返回的结果是个promise对象。\r\n  return new Promise((resolve, reject) => {\r\n    // 发起fetch请求，fetch请求的形参是接收上层函数的形参\r\n    OriginFetch(input, init)\r\n      .then((res) => {\r\n        // interceptorsRes是拦截响应结果的拦截处理函数集合\r\n        interceptorsRes.forEach((item) => {\r\n          // 拦截器对响应结果做处理，把处理后的结果返回给响应结果。\r\n          res = item(res);\r\n        });\r\n        // 将拦截器处理后的响应结果resolve出去\r\n        resolve(res);\r\n      })\r\n      .catch((err) => {\r\n        // interceptorsResError是拦截响应错误结果的拦截处理函数集合\r\n        interceptorsResError.forEach((item) => {\r\n          // 拦截器对响应错误结果做处理，把处理后的结果返回给响应结果。\r\n          err = item(err);\r\n        });\r\n        reject(err);\r\n      });\r\n  });\r\n}\r\n\r\n// interceptors拦截器提供request和response两种拦截器功能。\r\n// 可以通过request和response的use方法来绑定两种拦截器的处理函数。\r\n// use方法接收两个参数，参数为一个callback函数，callback函数用来作为拦截器的成功处理函数，errorCallback作为错误处理函数\r\n// request.use方法会把callback放在interceptorsReq中，等待执行。\r\n// response.use方法会把callback放在interceptorsRes中，等待执行。\r\n// 拦截器的处理函数callback接收一个参数。\r\n// request拦截器的callback接收的是请求发起前的config；\r\n// response拦截器的callback接收的是网络请求的response结果。\r\nconst interceptors = {\r\n  request: {\r\n    use(callback, errorCallback) {\r\n      interceptorsReq.push(callback);\r\n      errorCallback && interceptorsReqError.push(errorCallback);\r\n    },\r\n  },\r\n  response: {\r\n    use(callback, errorCallback) {\r\n      interceptorsRes.push(callback);\r\n      errorCallback && interceptorsResError.push(errorCallback);\r\n    },\r\n  },\r\n};\r\n\r\nexport { Cfetch, interceptors };\r\n"},"lineCount":null}},"error":null,"hash":"bd9ea3d4b26721825756d40b95f751e5","cacheData":{"env":{}}}